	<xc:ccLayout>
		<xp:this.facets>
			<xc:ccMenu xp:key="LeftColumn" />
		</xp:this.facets>
		<xp:panel>
			
			
			<div role="form">
  <div class="form-group">
    <label for="email">#{dao.dbpath}</label><br/>
    <xp:text escape="true" id="computedField1" value="#{dao.dbpath}" />
  </div>
  
  <div class="form-group">
    <label for="pwd">#{dao.server}</label><br/>
   <xp:text escape="true" id="computedField2" value="#{dao.server}" />
