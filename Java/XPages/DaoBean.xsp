  <div class="form-group">
    <label for="pwd">#{dao.filepath}</label><br/>
   <xp:text escape="true" id="computedField3" value="#{dao.filepath}" />
  </div>
  
  
</div>
		</xp:panel>
	</xc:ccLayout>
</xp:view>
